{
  "scopeName": "source.colorzing",
  "patterns": [
    {
      "name": "comment.line.double-dash.colorzing",
      "match": "--.*$"
    },
    {
      "name": "comment.block.colorzing",
      "begin": "\\[\\*",
      "end": "\\*\\]",
      "patterns": [
        {
          "name": "comment.block.documentation.colorzing",
          "match": "\\*\\*[^*]*\\*\\*"
        }
      ]
    },
    {
      "name": "keyword.control.import.colorzing",
      "match": "\\bimport\\[.*?\\]\\b"
    },
    {
      "name": "keyword.control.colorzing",
      "match": "\\b(if|then|else|log)\\b"
    },
    {
      "name": "entity.name.function.colorzing",
      "match": "\\b(LogLevel|Color|Math|Noise)\\b"
    },
    {
      "name": "constant.numeric.colorzing",
      "match": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "name": "string.quoted.double.colorzing",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "variable.parameter.colorzing",
      "match": "\\b(float|int|bool|LogLevel|Color)\\b"
    }
  ],
  "repository": {},
  "scopeName": "source.colorzing"
}
